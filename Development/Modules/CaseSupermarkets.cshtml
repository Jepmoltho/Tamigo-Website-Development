<style>
.container-responsive {
  width: 100%;
  padding-right: 20px;
  padding-left: 20px;
  margin-right: auto;
  margin-left: auto; }
  @@media (min-width: 576px) {
    .container-responsive {
      max-width: 920px; } }
  @@media (min-width: 768px) {
    .container-responsive {
      max-width: 920px; } }
  @@media (min-width: 992px) {
    .container-responsive {
      max-width: 960px; } }
